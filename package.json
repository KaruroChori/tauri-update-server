{
  "type": "module",
  "name": "tauri-update-server",
  "version": "1.0.0",
  "description": "Minimalistic server to deploy tauri updates with licence control.",
  "main": "main.js",
  "license": "MIT",
  "dependencies": {
    "sequelize": "^6.19.0",
    "sequelize-auto": "^0.8.8",
    "sqlite3": "^5.0.8"
  },

  "scripts": {
    "start": "node main.js",
    "db-auto": "sequelize-auto -i './models' -h localhost -u d  --dialect sqlite -d './private/db' -l esm"
  }
